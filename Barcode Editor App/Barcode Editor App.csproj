<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net7.0-windows</TargetFramework>
    <RootNamespace>Barcode_Editor_App</RootNamespace>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="BarcodeNet" Version="0.0.2" />
    <PackageReference Include="BarcodeNet.ImageSharp" Version="0.0.2" />
    <PackageReference Include="BarcodeNet.runtime.windows" Version="1.0.2-alpha.7" />
    <PackageReference Include="GerFern.MathOperations" Version="0.0.1" />
    <PackageReference Include="MaterialSkin.2" Version="2.3.1" />
  </ItemGroup>

</Project>